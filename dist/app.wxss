.oneline {
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
view,
scroll-view,
label,
picker,
text,
form,
button,
input,
swiper,
movable-view,
cover-view,
icon,
rich-text,
progress,
checkbox,
picker-view,
radio,
slider,
switch,
textarea,
navigator,
audio,
image,
video,
camera,
live-player,
live-pusher,
map,
canvas,
web-view {
  box-sizing: border-box;
}
.clearfix:before,
.clearfix:after {
  display: table;
  content: "";
  clear: both;
}
.pull-left {
  float: left;
}
.pull-right {
  float: right;
}
.hidden {
  display: none;
}
.h1 {
  margin: 20rpx 0;
  font-size: 18px;
  font-weight: bolder;
  text-align: center;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.h4 {
  margin: 20rpx 0;
  font-size: 12px;
  font-weight: bold;
  text-align: right;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
